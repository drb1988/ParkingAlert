extends dashboard-layout
block content
  .content-wrapper
    // Main content
    section.content
      // Small boxes (Stat box)
      .row
        .col-lg-3.col-xs-6
          // small box
          .small-box.bg-aqua(data-toggle='modal', data-target='#MapModal')
            .inner
              h3 Map
              p Heatmap region.
            .icon
              // i.ion.ion-ios-location
              i.ion.ion-ios-navigate
            a.small-box-footer(href='#')
              | See Map
              i.fa.fa-arrow-circle-right
          #MapModal.modal.fade(role='dialog' )
            .modal-dialog(style='max-width: 1200px; width: 90%;')
              // Modal content
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal') ×
                  h4.modal-title Friendly
                .modal-body
                  iframe(src="/web-routes/heatmaps_ajax" style='width:100%;height:100%' frameBorder="0")
                    p Your browser does not support iframes./p
                  // include heatmap_ajax
                .modal-footer
                  button.btn.btn-default(type='button', data-dismiss='modal') Close 
        // ./col
        .col-lg-3.col-xs-6#ChartModalClick
          // small box
          .small-box.bg-green(data-toggle='modal', data-target='#ChartModal')
            .inner
              h3
                | Statistics
              p View and print statistics.
            .icon
              i.ion.ion-stats-bars
            a.small-box-footer(href='#')
              | View statistics
              i.fa.fa-arrow-circle-right
          #ChartModal.modal.fade(role='dialog' )
            .modal-dialog(style='max-width: 1200px; width: 90%;')
              // Modal content
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal') ×
                  h4.modal-title Friendly
                .modal-body
                  iframe(src="/web-routes/stats" style='width:100%;height:100%' frameBorder="0")
                    p Your browser does not support iframes./p
                  // include heatmap_ajax
                .modal-footer
                  button.btn.btn-default(type='button', data-dismiss='modal') Close 
        // ./col
        .col-lg-3.col-xs-6
          // small box
          .small-box.bg-yellow(data-toggle='modal', data-target='#AdminModal')
            .container(style='color: #000000')
            .inner
              h3 Admin
              p Add admin.
            .icon
              i.ion.ion-person-add
            a.small-box-footer(href='#')
              | Add admin
              i.fa.fa-arrow-circle-right
          #AdminModal.modal.fade(role='dialog' )
            .modal-dialog(style='max-width: 1200px; width: 90%;')
              // Modal content
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal') ×
                  h4.modal-title Friendly
                .modal-body
                  iframe(src="/web-routes/add/admin" style='width:100%;height:100%' frameBorder="0")
                    p Your browser does not support iframes./p
                  // include heatmap_ajax
                .modal-footer
                  button.btn.btn-default(type='button', data-dismiss='modal') Close 
        // ./col
        .col-lg-3.col-xs-6
          // small box
          .small-box.bg-red(data-toggle='modal', data-target='#UsersModal')
            .inner
              h3 Users
              p Android and iOS users of the application.
            .icon
              i.ion.ion-ios-people
            a.small-box-footer(href='#')
              | View users
              i.fa.fa-arrow-circle-right
          #UsersModal.modal.fade(role='dialog' )
            .modal-dialog(style='max-width: 1200px; width: 90%;')
              // Modal content
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal') ×
                  h4.modal-title Friendly's users
                .modal-body
                  iframe(src="/web-routes/users" style='width:100%;height:100%' frameBorder="0")
                    p Your browser does not support iframes./p
                  // include heatmap_ajax
                .modal-footer
                  button.btn.btn-default(type='button', data-dismiss='modal') Close 
        .col-lg-3.col-xs-6
          // small box
          .small-box.bg-purple(data-toggle='modal', data-target='#QRGeneratorModal')
            .inner
              h3 QR Code
              p Generate and print QR code.
            .icon
              i.ion.ion-qr-scanner
            a.small-box-footer(href='#')
              | View generator
              i.fa.fa-arrow-circle-right
          #QRGeneratorModal.modal.fade(role='dialog' )
            .modal-dialog(style='max-width: 1200px; width: 90%;')
              // Modal content
              .modal-content
                .modal-header
                  button.close(type='button', data-dismiss='modal') ×
                  h4.modal-title Utilizatori Friendly
                .modal-body
                  iframe(src="/web-routes/qr" style='width:100%;height:100%' frameBorder="0")
                    p Your browser does not support iframes./p
                  // include heatmap_ajax
                .modal-footer
                  button.btn.btn-default(type='button', data-dismiss='modal') Close 
        // ./col
      // /.row
      // Main row
      .row
        // Left col
        section.col-lg-7.connectedSortable.hidden
          // Custom tabs (Charts with tabs)
          .nav-tabs-custom
            // Tabs within a box
           .box.box-solid.bg-green-gradient
            .box-header
              i.fa.fa-calendar
              h3.box-title Calendar
              // tools box
              .pull-right.box-tools
                button.btn.btn-success.btn-sm(type='button', data-widget='collapse')
                  i.fa.fa-minus
                button.btn.btn-success.btn-sm(type='button', data-widget='remove')
                  i.fa.fa-times
              // /. tools
            // /.box-header
            .box-body.no-padding
              // The calendar
              #calendar(style='width: 100%')
            // /.box-body
          // /.nav-tabs-custom
          .box.box-primary
            // /.box-header
            .box-body
            // /.box-body
            .box-footer.clearfix.no-border
          // /.box
          // quick email widget
        // /.Left col
        // right col (We are only adding the ID to make the widgets sortable)
        section.col-lg-5.connectedSortable.hidden
          .nav-tabs-custom
            // Tabs within a box
           .box.box-solid.bg-yellow-gradient
            .box-header
              i.fa.fa-calendar
              h3.box-title Ultimele notificari
            // /.box-header
            .box-body.no-padding
              // The calendar
              .row
                .col-md-12
                  // button sss
            // /.box-body
          // /.nav-tabs-custom
          .box.box-primary
            // /.box-header
            .box-body
            // /.box-body
            .box-footer.clearfix.no-border
          // /.box
          // /.box
          // Calendar
          .box.box-solid.bg-green-gradient
          // /.box
        // right col
      // /.row (main row)
    // /.content
  // /.content-wrapper
  footer.main-footer
    .pull-right.hidden-xs
      b Version
      |  2.3.6
    strong
      | Copyright © 2016 
      a(href='#') Friendly
      | .
    |  All rights
    |     reserved.
  // Control Sidebar
  aside.control-sidebar.control-sidebar-dark
    // Create the tabs
    ul.nav.nav-tabs.nav-justified.control-sidebar-tabs
      li
        a(href='#control-sidebar-home-tab', data-toggle='tab')
          i.fa.fa-home
      li
        a(href='#control-sidebar-settings-tab', data-toggle='tab')
          i.fa.fa-gears
    // Tab panes
    .tab-content
      // Home tab content
      #control-sidebar-home-tab.tab-pane
        h3.control-sidebar-heading Recent Activity
        ul.control-sidebar-menu
          li
            a(href='javascript:void(0)')
              i.menu-icon.fa.fa-birthday-cake.bg-red
              .menu-info
                h4.control-sidebar-subheading Langdon's Birthday
                p Will be 23 on April 24th
          li
            a(href='javascript:void(0)')
              i.menu-icon.fa.fa-user.bg-yellow
              .menu-info
                h4.control-sidebar-subheading Frodo Updated His Profile
                p New phone +1(800)555-1234
          li
            a(href='javascript:void(0)')
              i.menu-icon.fa.fa-envelope-o.bg-light-blue
              .menu-info
                h4.control-sidebar-subheading Nora Joined Mailing List
                p nora@example.com
          li
            a(href='javascript:void(0)')
              i.menu-icon.fa.fa-file-code-o.bg-green
              .menu-info
                h4.control-sidebar-subheading Cron Job 254 Executed
                p Execution time 5 seconds
        // /.control-sidebar-menu
        h3.control-sidebar-heading Tasks Progress
        ul.control-sidebar-menu
          li
            a(href='javascript:void(0)')
              h4.control-sidebar-subheading
                | Custom Template Design
                span.label.label-danger.pull-right 70%
              .progress.progress-xxs
                .progress-bar.progress-bar-danger(style='width: 70%')
          li
            a(href='javascript:void(0)')
              h4.control-sidebar-subheading
                | Update Resume
                span.label.label-success.pull-right 95%
              .progress.progress-xxs
                .progress-bar.progress-bar-success(style='width: 95%')
          li
            a(href='javascript:void(0)')
              h4.control-sidebar-subheading
                | Laravel Integration
                span.label.label-warning.pull-right 50%
              .progress.progress-xxs
                .progress-bar.progress-bar-warning(style='width: 50%')
          li
            a(href='javascript:void(0)')
              h4.control-sidebar-subheading
                | Back End Framework
                span.label.label-primary.pull-right 68%
              .progress.progress-xxs
                .progress-bar.progress-bar-primary(style='width: 68%')
        // /.control-sidebar-menu
      // /.tab-pane
      // Stats tab content
      #control-sidebar-stats-tab.tab-pane Stats Tab Content
      // /.tab-pane
      // Settings tab content
      #control-sidebar-settings-tab.tab-pane
        form(method='post')
          h3.control-sidebar-heading General Settings
          .form-group
            label.control-sidebar-subheading
              | Report panel usage
              input.pull-right(type='checkbox', checked='')
            p
              | Some information about this general settings option
          // /.form-group
          .form-group
            label.control-sidebar-subheading
              | Allow mail redirect
              input.pull-right(type='checkbox', checked='')
            p
              | Other sets of options are available
          // /.form-group
          .form-group
            label.control-sidebar-subheading
              | Expose author name in posts
              input.pull-right(type='checkbox', checked='')
            p
              | Allow the user to show his name in blog posts
          // /.form-group
          h3.control-sidebar-heading Chat Settings
          .form-group
            label.control-sidebar-subheading
              | Show me as online
              input.pull-right(type='checkbox', checked='')
          // /.form-group
          .form-group
            label.control-sidebar-subheading
              | Turn off notifications
              input.pull-right(type='checkbox')
          // /.form-group
          .form-group
            label.control-sidebar-subheading
              | Delete chat history
              a.text-red.pull-right(href='javascript:void(0)')
                i.fa.fa-trash-o
          // /.form-group
      // /.tab-pane
  // /.control-sidebar
  //
    Add the sidebar's background. This div must be placed
    immediately after the control sidebar
  .control-sidebar-bg
// ./wrapper

block scripts
    // jQuery 2.2.3
    script(src='/plugins/jQuery/jquery-2.2.3.min.js')
    // jQuery UI 1.11.4
    script(src='https://code.jquery.com/ui/1.11.4/jquery-ui.min.js')
    // Resolve conflict in jQuery UI tooltip with Bootstrap tooltip
    script.
      $.widget.bridge('uibutton', $.ui.button);
    // Bootstrap 3.3.6
    script(src='/bootstrap/js/bootstrap.min.js')
    // Morris.js charts
    script(src='/plugins/morris/morris.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js')
    link(rel='stylesheet', href='//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.css')
    // Sparkline
    script(src='/plugins/sparkline/jquery.sparkline.min.js')
    // jvectormap
    script(src='/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js')
    script(src='/plugins/jvectormap/jquery-jvectormap-world-mill-en.js')
    // jQuery Knob Chart
    script(src='/plugins/knob/jquery.knob.js')
    // daterangepicker
    script(src='https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js')
    script(src='/plugins/daterangepicker/daterangepicker.js')
    // datepicker
    script(src='/plugins/datepicker/bootstrap-datepicker.js')
    // Bootstrap WYSIHTML5
    script(src='/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js')
    // Slimscroll
    script(src='/plugins/slimScroll/jquery.slimscroll.min.js')
    // FastClick
    script(src='/plugins/fastclick/fastclick.js')
    // AdminLTE App
    script(src='/dist/js/app.min.js')
    // AdminLTE dashboard demo (This is only for demo purposes)
    script(src='/dist/js/pages/dashboard.js')
    // AdminLTE for demo purposes
    script(src='/dist/js/demo.js')

    